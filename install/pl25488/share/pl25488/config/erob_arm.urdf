  <!-- Fixed joint attaching the inertial child to the root -->
  <joint name="base_link_fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_link_inertial"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>



  <material name="#ffffffff">
    <color rgba="0.8000 0.40000 0 1.0000"/>
  </material>
    <material name="base">
    <color rgba="0.40000 0.40000 0.40000 1.0000"/>
  </material>
    <!-- Tool0 frame -->
  <link name="tool0">
    <visual>
      <!-- keep a visible marker at the tool frame (unchanged) -->
      <origin xyz="0 0.01 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.010"/> <!-- Small sphere as marker -->
      </geometry>
      <material name="tool_marker">
        <color rgba="1 0 0 1"/> <!-- Red marker -->
      </material>
    </visual>
    <!-- Collision geometry for tool - extended sphere to detect table/base collisions -->
    <collision>
      <!-- Extended forward along Y axis (tool direction) - further to detect table at 45mm -->
      <origin xyz="0 0.04 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.03"/> <!-- 30mm radius - ensures detection of table at 45mm height -->
      </geometry>
    </collision>
  </link>

  <joint name="tool0" type="fixed">
    <parent link="Link_6"/>
    <child link="tool0"/>
    <!-- Move the tool origin a small amount away from Link_6 to avoid an immediate self-collision.
         If you have the exact tool transform, use that instead. -->
    <origin xyz="0.0 0.01 0" rpy="0.0 0.0 0.0"/>
  </joint>

  <material name="tool_marker">
    <color rgba="1 0 0 1"/>
  </material>

  <!-- TABLE: Fixed collision object representing the work surface -->
  <!-- This allows MoveIt to automatically avoid the table without manual Z limits -->
  <!-- Table top at Z=0.08m (80mm above base) to provide larger safety margin -->
  <link name="table">
    <visual>
      <origin xyz="0 0 0.055" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 2.0 0.05"/> <!-- 2m x 2m x 5cm table -->
      </geometry>
      <material name="table_material">
        <color rgba="0.6 0.4 0.2 0.8"/> <!-- Brown with some transparency -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.055" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 2.0 0.05"/> <!-- 2m x 2m x 5cm table -->
      </geometry>
    </collision>
  </link>

  <joint name="table_joint" type="fixed">
    <parent link="base_link"/>
    <child link="table"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <material name="table_material">
    <color rgba="0.6 0.4 0.2 0.8"/>
  </material>

  <!-- Ensure joint limits here match those in ros2_control.xacro and SRDF for correct planner behavior -->
